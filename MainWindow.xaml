<Window x:Class="OSNK_1_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OSNK_1_wpf"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="Тренажёр" Height="450" Width="800" ResizeMode="CanMinimize">
    <Grid x:Name="grid1" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="411*"></RowDefinition>
            <RowDefinition Height="23*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="targetTB" HorizontalAlignment="Left" Margin="203,87,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="163" Height="51"/>
        <Button x:Name="menuB" Content="Выбрать песню" HorizontalAlignment="Left" Margin="435,10,0,0" VerticalAlignment="Top" Width="321" Height="45"/>
        <ComboBox x:Name="partB" HorizontalAlignment="Left" Margin="435,172,0,0" VerticalAlignment="Top" Width="321" Height="47"/>
        <ComboBox x:Name="minusB" HorizontalAlignment="Left" Margin="435,87,0,0" VerticalAlignment="Top" Width="321" Height="48"/>
        <CheckBox x:Name="listenCB" Content="CheckBox" HorizontalAlignment="Left" Margin="435,310,0,0" VerticalAlignment="Top" Height="33" Width="191"/>
        <CheckBox x:Name="logCB" Content="CheckBox" HorizontalAlignment="Left" Margin="435,348,0,0" VerticalAlignment="Top" Height="31" Width="191"/>
        <Label x:Name="tempL" Content="Label" HorizontalAlignment="Left" Margin="203,177,0,0" VerticalAlignment="Top" Height="50" Width="163"/>
        <Button x:Name="startB" Content="Начать запись" HorizontalAlignment="Left" Margin="40,305,0,0" VerticalAlignment="Top" Width="326" Height="50" />
        <ComboBox x:Name="modeCB" HorizontalAlignment="Left" Margin="285,225,0,0" VerticalAlignment="Top" Width="81" Height="23" SelectedIndex="0">
            <ComboBoxItem Content="Ноты"/>
            <ComboBoxItem Content="Частоты"/>
        </ComboBox>

    </Grid>
</Window>
